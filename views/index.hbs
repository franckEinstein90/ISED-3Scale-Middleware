
<div class="w3-main" style="margin-left:160px;margin-top:43px;margin-right:10px">

<!-- Header -->

   <header class="w3-container" style="padding-top:22px">
     <div class="w3-panel">
     <p>app status: <span id="appStatus">{{{state}}}</span></p>
     </div>
   </header>

     <div class="w3-container" ID="tenantCards">
		{{#each tenants}}
		<div class="w3-card ws-margin-16" ID="{{this.name}}">
			<header class="w3-container w3-blue">
				<div class="w3-left"><h4>Tenant: {{this.name}}</h4></div>
			</header>

			<div class="w3-container">
				<p>Services: <UL>
					{{#each services}}
					<LI>{{this}}</li>
					{{/each}}
				</UL></p>
			</div>
			<footer class="w3-container w3-blue">
				<div class="w3-center w3-padding-16">Last Update: {{this.lastUpdate}}</div>
			</footer>
		</div>
		<br>	
		{{/each}}
	     </div>
</div>


